<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<MessageLogging async="false" continueOnError="true" enabled="true" name="cf_Log_Prod">
    <DisplayName>cf_Log_Prod</DisplayName>
    <Syslog>
        <Message>{logToSplunk}||proxyRespStart={client.received.end.timestamp}||proxyRespEnd={client.sent.end.timestamp}</Message>
        <Host>spksys.t-mobile.com</Host>
        <Port>1999</Port>
        <Protocol>TCP</Protocol>
        <SSLInfo>
            <Enabled>true</Enabled>
        </SSLInfo>
    </Syslog>
</MessageLogging>